<section class="content-header">
    <h1>
        @TextosPaginas.Home_ViagemAerea

    </h1>
    <ol class="breadcrumb">

        <li>@TextosPaginas.Home_Menu</li>
        <li class="active">@TextosPaginas.Home_ViagemAerea</li>
    </ol>

</section>
<!-- Main content -->
<section class="content" ng-init="itemViagemAerea.load()">
    <!-- Default box -->
    <!-- general form elements -->
    <div class="box box-primary">
        <div class="overlay" ng-show="itemViagemAerea.loading">
            <i class="fa fa-refresh fa-spin text-gray"></i>
        </div>

        <div class="box collapsed-box">
            <div class="box-header with-border">
                <h3 class="box-title"><i class="fa fa-filter"></i>&nbsp;@TextosPaginas.Foto_Filtros</h3>

                <div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-plus"></i></button>
                </div>
            </div>
            <div class="box-body">
                <div class="row">

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.ViagemAerea_CompanhiaAerea</label>
                        <input type="text" class="form-control" ng-model="itemViagemAerea.filtro.Nome" maxlength="100" empty-to-null>
                    </div>

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.ViagemAerea_Tipo</label>
                        <select ng-model="itemViagemAerea.itemTipo" class="form-control" ng-options="obj.Descricao for obj in itemViagemAerea.ListaTipo track by obj.Codigo">
                            <option value="">@TextosPaginas.Selecione</option>
                        </select>
                    </div>

                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Foto_DataDe</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemViagemAerea.filtro.DataInicioDe" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemViagemAerea.filtro.DataInicioDe"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Foto_DataAte</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemViagemAerea.filtro.DataInicioAte" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemViagemAerea.filtro.DataInicioAte"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                            </ul>
                        </div>
                    </div>

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.ViagemAerea_CidadeOrigem</label>
                        <select ng-model="itemViagemAerea.itemCidadeOrigem" class="form-control" ng-options="obj.Nome for obj in itemViagemAerea.ListaCidades track by obj.Identificador">
                            <option value="">@TextosPaginas.Selecione</option>
                        </select>
                    </div>

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.ViagemAerea_CidadeDestino</label>
                        <select ng-model="itemViagemAerea.itemCidadeDestino" class="form-control" ng-options="obj.Nome for obj in itemViagemAerea.ListaCidades track by obj.Identificador">
                            <option value="">@TextosPaginas.Selecione</option>
                        </select>
                    </div>

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Atracao_Situacao</label>

                        <div class="form-inline">

                            <label class="radio">
                                <input type="radio" class="radio-inline" name="radSituacao" value="1" ng-model="itemViagemAerea.filtro.Situacao" />

                                <label style="padding-top:5px">

                                    @TextosPaginas.Hotel_Presente
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radSituacao" value="2" ng-model="itemViagemAerea.filtro.Situacao" /><label>
                                    @TextosPaginas.Hotel_Passado
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radSituacao" value="3" ng-model="itemViagemAerea.filtro.Situacao" />
                                <label>
                                    @TextosPaginas.Hotel_Futuro
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radSituacao" value="4" ng-model="itemViagemAerea.filtro.Situacao" />
                                <label>
                                    @TextosPaginas.Hotel_Todos
                                </label>
                            </label>
                        </div>

                    </div>
                </div>




            </div><!-- /.box-body -->
            <div class="box-footer">


                <button class="btn btn-primary  pull-right" ng-click="itemViagemAerea.filtraDado()">
                    <span class="fa fa-search" aria-hidden="true"></span> @TextosPaginas.Pesquisar
                </button>

            </div>
        </div>

        <div class="box-footer">
            <a role="button" class="btn btn-primary " title="@TextosPaginas.Adicionar" ng-click="itemViagemAerea.CriarNovaViagemAerea()">
                <span class="fa fa-plus" aria-hidden="true"></span>
            </a>
        </div>
    </div><!-- /.box -->



    <div class="box">
        <div class="overlay" ng-show="itemViagemAerea.loading">
            <i class="fa fa-refresh fa-spin text-gray"></i>
        </div>
        <div ng-repeat='item in itemViagemAerea.ListaDados' class="box " ng-class="{ 'collapsed-box': item.Identificador }" class-validation ng-controller="ViagemAereaEditCtrl as itemEdit">
            <div class="overlay" ng-show="itemEdit.loading">
                <i class="fa fa-refresh fa-spin text-gray"></i>
            </div>

            <div class="box-header with-border">
                <a data-widget="collapse" data-toggle="tooltip" ng-click="itemEdit.load(item)" style="cursor:pointer">
                    <i class="fa fa-plus" ng-class="{ 'fa-minus': !item.Identificador }"></i>


                    <h3 class="box-title">{{item.Descricao}}</h3>

                </a>
            </div>

            <div class="box-body" ng-init="itemEdit.ajustaInicio(item)">

                <div class="row" ng-show="itemEdit.messages.length > 0">
                    <div class="col-md-12">
                        <div class="callout callout-danger">
                            <h4>@TextosPaginas.Geral_ValidarErros</h4>
                            <ul>
                                <li ng-repeat="erro in itemEdit.messages">
                                    <span ng-bind="erro.Mensagem"></span>
                                </li>
                            </ul>

                        </div>
                    </div>

                </div>
                <div class="row">

                    <div class="col-md-6 " ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Descricao}">
                        <label>@TextosPaginas.ViagemAerea_Descricao</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.Descricao" maxlength="200" empty-to-null>
                    </div>
                </div>
                    <div class="row">

                        <div class="col-md-6 " ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.CompanhiaAerea}">
                            <label>@TextosPaginas.ViagemAerea_CompanhiaAerea</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.CompanhiaAerea" maxlength="50" empty-to-null>
                        </div>

                        <div class="col-md-3" ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Tipo}">
                            <label>@TextosPaginas.ViagemAerea_Tipo</label>
                            <select ng-model="itemEdit.itemTipoViagem" class="form-control" ng-options="obj.Descricao for obj in itemViagemAerea.ListaTipo track by obj.Codigo">
                                <option value="">@TextosPaginas.Selecione</option>
                            </select>

                        </div>

                        <div class="col-md-3" ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.DataPrevista}">
                            <label>@TextosPaginas.ViagemAerea_DataPrevista</label>
                            <div class="dropdown">
                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                    <div class="input-group">
                                        <input date-input ng-model="itemEdit.itemViagemAerea.DataPrevista" class="form-control" input-mask="{mask: 'd/m/y'}"
                                               datapattern="dd/MM/yyyy">
                                        <span class="input-group-addon">
                                            <i class="glyphicon glyphicon-calendar"></i>
                                        </span>
                                    </div>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                    <datetimepicker data-ng-model="itemEdit.itemViagemAerea.DataPrevista"
                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <h3>@TextosPaginas.ViagemAerea_Origem</h3>
                        </div>
                        <div class="col-md-6 col-xs-6" ng-class="{ 'has-error':itemEdit.itemPontoOrigem.CamposInvalidos.Aeroporto}">
                            <label>@TextosPaginas.ViagemAerea_Aeroporto</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemPontoOrigem.Aeroporto" maxlength="100" empty-to-null ng-if="(!itemEdit.itemPontoOrigem.Latitude || !itemEdit.itemPontoOrigem.Longitude) && !itemEdit.position.lat">
                            <input ng-if="(itemEdit.itemPontoOrigem.Latitude && itemEdit.itemPontoOrigem.Longitude) || itemEdit.position.lat" type="text" ng-model="itemEdit.itemPontoOrigem.Aeroporto" uib-typeahead="address.name for address in itemEdit.CarregarDadosGoogleApi($viewValue,itemEdit.itemPontoOrigem) " typeahead-loading="itemEdit.loadingLocations" class="form-control" typeahead-on-select="itemEdit.SelecionarPlacesGoogle($item, $model, $label,itemEdit.itemPontoOrigem)">

                        </div>
                        <div class="col-md-2">
                            <br />
                            <a role="button" class="btn btn-primary " title="@TextosPaginas.Posicao" ng-click="itemEdit.SelecionarPosicao(itemEdit.itemPontoOrigem)">
                                <span class="fa fa-map-marker" aria-hidden="true"></span>
                            </a>
                        </div>
                        <div class="col-md-2 col-xs-6">
                            <label>@TextosPaginas.ViagemAerea_Chegada</label>
                            <input type="checkbox" ng-model="itemEdit.itemPontoOrigem.ChegadaPonto" ng-click="itemEdit.AjustarHoraChegada(itemEdit.itemPontoOrigem);" class="checkbox">
                        </div>
                        <div class="col-md-2  col-xs-6" ng-if="itemEdit.itemPontoOrigem.ChegadaPonto">
                            <label>@TextosPaginas.ViagemAerea_Partida</label>
                            <input type="checkbox" ng-model="itemEdit.itemPontoOrigem.PartidaPonto" ng-click="itemEdit.AjustarHoraPartida(itemEdit.itemPontoOrigem);" class="checkbox">
                        </div>

                        <div class="col-md-3" ng-if="itemEdit.itemPontoOrigem.ChegadaPonto">
                            <label>@TextosPaginas.ViagemAerea_DataChegada</label>
                            <div class="dropdown">
                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                    <div class="input-group">
                                        <input date-input ng-model="itemEdit.itemPontoOrigem.DataChegada" class="form-control" input-mask="{mask: 'd/m/y'}"
                                               datapattern="dd/MM/yyyy">
                                        <span class="input-group-addon">
                                            <i class="glyphicon glyphicon-calendar"></i>
                                        </span>
                                    </div>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                    <datetimepicker data-ng-model="itemEdit.itemPontoOrigem.DataChegada"
                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()   }" />
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-3" ng-if="itemEdit.itemPontoOrigem.ChegadaPonto">
                            <label>&nbsp;</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemPontoOrigem.strHoraChegada" input-mask="{mask: 'h:s:s'}" empty-to-null>

                        </div>

                        <div class="col-md-3" ng-if="itemEdit.itemPontoOrigem.PartidaPonto">
                            <label>@TextosPaginas.ViagemAerea_DataPartida</label>
                            <div class="dropdown">
                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                    <div class="input-group">
                                        <input date-input ng-model="itemEdit.itemPontoOrigem.DataPartida" class="form-control" input-mask="{mask: 'd/m/y'}"
                                               datapattern="dd/MM/yyyy">
                                        <span class="input-group-addon">
                                            <i class="glyphicon glyphicon-calendar"></i>
                                        </span>
                                    </div>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                    <datetimepicker data-ng-model="itemEdit.itemPontoOrigem.DataPartida"
                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-3" ng-if="itemEdit.itemPontoOrigem.PartidaPonto">
                            <label>&nbsp;</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemPontoOrigem.strHoraPartida" input-mask="{mask: 'h:s:s'}" empty-to-null>

                        </div>

                    </div>

                    <div class="row">
                        <div class="col-md-12">
                            <h3>@TextosPaginas.ViagemAerea_Destino</h3>
                        </div>
                        <div class="col-md-6 col-xs-6" ng-class="{ 'has-error':itemEdit.itemPontoDestino.CamposInvalidos.Aeroporto}">
                            <label>@TextosPaginas.ViagemAerea_Aeroporto</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemPontoDestino.Aeroporto" maxlength="100" empty-to-null ng-if="(!itemEdit.itemPontoDestino.Latitude || !itemEdit.itemPontoDestino.Longitude) && !itemEdit.position.lat">
                            <input ng-if="(itemEdit.itemPontoDestino.Latitude && itemEdit.itemPontoDestino.Longitude) || itemEdit.position.lat" type="text" ng-model="itemEdit.itemPontoDestino.Aeroporto" uib-typeahead="address.name for address in itemEdit.CarregarDadosGoogleApi($viewValue,itemEdit.itemPontoDestino) " typeahead-loading="itemEdit.loadingLocations" class="form-control" typeahead-on-select="itemEdit.SelecionarPlacesGoogle($item, $model, $label,itemEdit.itemPontoDestino)">

                        </div>
                        <div class="col-md-2 col-xs-6">
                            <br />
                            <a role="button" class="btn btn-primary " title="@TextosPaginas.Posicao" ng-click="itemEdit.SelecionarPosicao(itemEdit.itemPontoDestino)">
                                <span class="fa fa-map-marker" aria-hidden="true"></span>
                            </a>
                        </div>
                        <div class="col-md-2 col-xs-6">
                            <label>@TextosPaginas.ViagemAerea_Chegada</label>
                            <input type="checkbox" ng-model="itemEdit.itemPontoDestino.ChegadaPonto" ng-click="itemEdit.AjustarHoraChegada(itemEdit.itemPontoDestino);" class="checkbox">
                        </div>
                        <div class="col-md-2  col-xs-6" ng-if="itemEdit.itemPontoDestino.ChegadaPonto">
                            <label>@TextosPaginas.ViagemAerea_Partida</label>
                            <input type="checkbox" ng-model="itemEdit.itemPontoDestino.PartidaPonto" ng-click="itemEdit.AjustarHoraPartida(itemEdit.itemPontoDestino);" class="checkbox">
                        </div>

                        <div class="col-md-3" ng-if="itemEdit.itemPontoDestino.ChegadaPonto">
                            <label>@TextosPaginas.ViagemAerea_DataChegada</label>
                            <div class="dropdown">
                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                    <div class="input-group">
                                        <input date-input ng-model="itemEdit.itemPontoDestino.DataChegada" class="form-control" input-mask="{mask: 'd/m/y'}"
                                               datapattern="dd/MM/yyyy">
                                        <span class="input-group-addon">
                                            <i class="glyphicon glyphicon-calendar"></i>
                                        </span>
                                    </div>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                    <datetimepicker data-ng-model="itemEdit.itemPontoDestino.DataChegada"
                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()   }" />
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-3" ng-if="itemEdit.itemPontoDestino.ChegadaPonto">
                            <label>&nbsp;</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemPontoDestino.strHoraChegada" input-mask="{mask: 'h:s:s'}" empty-to-null>

                        </div>

                        <div class="col-md-3" ng-if="itemEdit.itemPontoDestino.PartidaPonto">
                            <label>@TextosPaginas.ViagemAerea_DataPartida</label>
                            <div class="dropdown">
                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                    <div class="input-group">
                                        <input date-input ng-model="itemEdit.itemPontoDestino.DataPartida" class="form-control" input-mask="{mask: 'd/m/y'}"
                                               datapattern="dd/MM/yyyy">
                                        <span class="input-group-addon">
                                            <i class="glyphicon glyphicon-calendar"></i>
                                        </span>
                                    </div>
                                </a>
                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                    <datetimepicker data-ng-model="itemEdit.itemPontoDestino.DataPartida"
                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-3" ng-if="itemEdit.itemPontoDestino.PartidaPonto">
                            <label>&nbsp;</label>
                            <input type="text" class="form-control" ng-model="itemEdit.itemPontoDestino.strHoraPartida" input-mask="{mask: 'h:s:s'}" empty-to-null>

                        </div>

                    </div>


                    <div class="row">
                        <div class="col-md-10 col-xs-6">
                            <h3>@TextosPaginas.ViagemAerea_Escala</h3>
                        </div>
                        <div class="col-md-2 col-xs-6">
                            <br />
                            <a role="button" class="btn btn-primary" title="@TextosPaginas.Adicionar" ng-click="itemEdit.AdicionarEscala()">
                                <span class="fa fa-plus" aria-hidden="true"></span>
                            </a>
                        </div>
                    </div>
                    <div class="row" ng-repeat="itemEscala in itemEdit.itemViagemAerea.Aeroportos" ng-if="itemEscala.TipoPonto==3 && !itemEscala.DataExclusao">
                        <div class="col-xs-12">
                            <div class="box">
                                <div class="box-body">
                                    <div class="row">
                                        <div class="col-md-6 col-xs-6" ng-class="{ 'has-error':itemEscala.CamposInvalidos.Aeroporto}">

                                            <label>@TextosPaginas.ViagemAerea_Aeroporto</label>
                                            <input type="text" class="form-control" ng-model="itemEscala.Aeroporto" maxlength="100" empty-to-null ng-if="(!itemEscala.Latitude || !itemEscala.Longitude) && !itemEdit.position.lat">
                                            <input ng-if="(itemEscala.Latitude && itemEscala.Longitude) || itemEdit.position.lat" type="text" ng-model="itemEscala.Aeroporto" uib-typeahead="address.name for address in itemEdit.CarregarDadosGoogleApi($viewValue,itemEscala) " typeahead-loading="itemEdit.loadingLocations" class="form-control" typeahead-on-select="itemEdit.SelecionarPlacesGoogle($item, $model, $label,itemEscala)">

                                        </div>
                                        <div class="col-md-2 col-xs-6">
                                            <br />
                                            <a role="button" class="btn btn-primary" title="@TextosPaginas.Posicao" ng-click="itemEdit.SelecionarPosicao(itemEscala)">
                                                <span class="fa fa-map-marker" aria-hidden="true"></span>
                                            </a>
                                            <a role="button" class="btn btn-danger" title="@TextosPaginas.Excluir" ng-click="itemEdit.ExcluirEscala(itemEscala)">
                                                <span class="fa fa-trash" aria-hidden="true"></span>
                                            </a>
                                        </div>
                                        <div class="col-md-2 col-xs-6">
                                            <label>@TextosPaginas.ViagemAerea_Chegada</label>
                                            <input type="checkbox" ng-model="itemEscala.ChegadaPonto" ng-click="itemEdit.AjustarHoraChegada(itemEscala);" class="checkbox">
                                        </div>
                                        <div class="col-md-2  col-xs-6" ng-if="itemEscala.ChegadaPonto">
                                            <label>@TextosPaginas.ViagemAerea_Partida</label>
                                            <input type="checkbox" ng-model="itemEscala.PartidaPonto" ng-click="itemEdit.AjustarHoraPartida(itemEscala);" class="checkbox">
                                        </div>

                                        <div class="col-md-3" ng-if="itemEscala.ChegadaPonto">
                                            <label>@TextosPaginas.ViagemAerea_DataChegada</label>
                                            <div class="dropdown">
                                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                                    <div class="input-group">
                                                        <input date-input ng-model="itemEscala.DataChegada" class="form-control" input-mask="{mask: 'd/m/y'}"
                                                               datapattern="dd/MM/yyyy">
                                                        <span class="input-group-addon">
                                                            <i class="glyphicon glyphicon-calendar"></i>
                                                        </span>
                                                    </div>
                                                </a>
                                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                                    <datetimepicker data-ng-model="itemEscala.DataChegada"
                                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()   }" />
                                                </ul>
                                            </div>
                                        </div>
                                        <div class="col-md-3" ng-if="itemEscala.ChegadaPonto">
                                            <label>&nbsp;</label>
                                            <input type="text" class="form-control" ng-model="itemEscala.strHoraChegada" input-mask="{mask: 'h:s:s'}" empty-to-null>

                                        </div>

                                        <div class="col-md-3" ng-if="itemEscala.PartidaPonto">
                                            <label>@TextosPaginas.ViagemAerea_DataPartida</label>
                                            <div class="dropdown">
                                                <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                                    <div class="input-group">
                                                        <input date-input ng-model="itemEscala.DataPartida" class="form-control" input-mask="{mask: 'd/m/y'}"
                                                               datapattern="dd/MM/yyyy">
                                                        <span class="input-group-addon">
                                                            <i class="glyphicon glyphicon-calendar"></i>
                                                        </span>
                                                    </div>
                                                </a>
                                                <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                                    <datetimepicker data-ng-model="itemEscala.DataPartida"
                                                                    data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                                                </ul>
                                            </div>
                                        </div>
                                        <div class="col-md-3" ng-if="itemEscala.PartidaPonto">
                                            <label>&nbsp;</label>
                                            <input type="text" class="form-control" ng-model="itemEscala.strHoraPartida" input-mask="{mask: 'h:s:s'}" empty-to-null>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>


                    <div class="row">
                        <div class="col-md-12">
                            <label>@TextosPaginas.ViagemAerea_Participantes</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-inline">
                                <label ng-repeat='itemParticipante in itemEdit.ListaParticipante' class="radio">
                                    <input type="checkbox" ng-model="itemParticipante.Selecionado">
                                    <label style="padding-top:5px">
                                        {{itemParticipante.Nome}}
                                    </label>
                                </label>

                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12" ng-if="itemEdit.VerificarParticipoViagemAerea() && itemEdit.itemPontoDestino.PartidaPonto">
                            <label>@TextosPaginas.Refeicao_Nota</label>
                            <div star-rating ng-model="itemEdit.ItemAvaliacao.Nota" max="5"></div>
                        </div>
                        <div class="col-md-12" ng-if="itemEdit.VerificarParticipoViagemAerea() && itemEdit.itemPontoDestino.PartidaPonto">
                            <label>@TextosPaginas.Refeicao_Comentario</label>
                            <textarea class="form-control" rows="5" ng-model="itemEdit.ItemAvaliacao.Comentario"></textarea>

                        </div>
                    </div>
                    <div class="row" ng-show="item.Identificador">
                        <div class="col-xs-12">
                            <ul class="nav nav-tabs">
                                <li class="active"><a data-toggle="tab" data-target="#custos{{item.Identificador}}" style="cursor:pointer">@TextosPaginas.Refeicao_Custos</a></li>
                            </ul>

                            <div class="tab-content" style="height:140px">
                                <div id="custos{{item.Identificador}}" class="tab-pane fade in active ">
                                    <div class="box" style="overflow-y:auto; height:140px">

                                        <table class="table-condensed " style="width:100%; ">
                                            <thead>
                                                <tr>
                                                    <th class="col-xs-3">
                                                        @TextosPaginas.Gasto_Descricao
                                                    </th>
                                                    <th class="col-xs-3">
                                                        @TextosPaginas.Gasto_Data
                                                    </th>
                                                    <th class="col-xs-3">
                                                        @TextosPaginas.Gasto_Moeda
                                                    </th>
                                                    <th class="col-xs-2">
                                                        @TextosPaginas.Gasto_Valor
                                                    </th>

                                                    <th class="col-xs-1">

                                                    </th>
                                                </tr>
                                            </thead>
                                            <tr ng-repeat='itemCusto in itemEdit.itemViagemAerea.Gastos' ng-if="!itemCusto.DataExclusao">
                                                <td>
                                                    {{itemCusto.ItemGasto.Descricao}}
                                                </td>
                                                <td>
                                                    {{itemCusto.ItemGasto.Data | date:'dd/MM/yyyy'}}
                                                </td>
                                                <td>
                                                    {{itemCusto.ItemGasto.MoedaSigla}}
                                                </td>
                                                <td>
                                                    {{itemCusto.ItemGasto.Valor | number:'2'}}
                                                </td>
                                                <td>
                                                    <button type="button" class="btn btn-danger small" ng-click="itemEdit.RemoverCusto( itemCusto)"><span class="fa fa-trash" aria-hidden="true"></span></button>

                                                </td>
                                            </tr>
                                        </table>

                                        <div class="box-footer">
                                            <a role="button" class="btn-primary btn-sm pull-right" title="@TextosPaginas.Adicionar" ng-click="itemEdit.AbrirGasto();">
                                                <span class="fa fa-plus" aria-hidden="true"></span>
                                            </a>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            <div class="box-footer">

                <a role="button" class="btn btn-primary pull-right" title="@TextosPaginas.Geral_Salvar" ng-click="itemEdit.save()">
                    <span class="fa fa-save" aria-hidden="true"></span>
                </a>

                <a role="button" class="btn btn-primary pull-right" title="@TextosPaginas.Excluir" ng-click="itemEdit.VerificarExclusao()">
                    <span class="fa fa-trash-o" aria-hidden="true"></span>
                </a>



            </div>
        </div>


    </div>


    <script type="text/ng-template" id="modalDelete.html">
        <div class="modal-header">
            <span>{{vmDelete.MensagemConfirmacao}}</span>
        </div>
        <div class="modal-body">
            <button type="button" class="btn btn-info btn-lg btn-block" ng-click="vmDelete.confirmar()">{{vmDelete.TextoBotaoOK}}</button>
            <button type="button" class="btn btn-default btn-lg btn-block" ng-click="vmDelete.close()">{{vmDelete.TextoBotaoCancel}}</button>
        </div>
    </script>


    <script type="text/ng-template" id="modalMapa.html">
        <div class="modal-body">
            <div class="box-body">

                <div class="row">
                    <div class="form-group  col-sm-8">
                        <label>@TextosPaginas.Posicao_Endereco</label>
                        <input places-auto-complete size=80
                               class="form-control"
                               ng-model="vmMapa.itemEndereco"
                               on-place-changed="vmMapa.selecionarEndereco()" /> 
                    </div>
                    <div class="form-group  col-sm-2">
                        <label>&nbsp;</label>
                        <button class="btn btn-primary btn-block " ng-click="vmMapa.centralizarEndereco()">
                            <span class="fa fa-search" aria-hidden="true"></span>
                        </button>
                    </div>
                    <div class="form-group  col-sm-2">
                        <label>&nbsp;</label>
                        <button class="btn btn-primary btn-block" ng-click="vmMapa.limparPosicao()">
                            <span class="fa fa-unlink" aria-hidden="true"></span>
                        </button>
                    </div>
                </div>
            </div>
            <ng-map zoom="13"
                    center="[{{vmMapa.lat}}, {{vmMapa.lng}}]" on-click="vmMapa.ajustaPosicao()">
                <marker ng-if="vmMapa.itemMarcador.Latitude && vmMapa.itemMarcador.Longitude" position="{{vmMapa.itemMarcador.Latitude}}, {{vmMapa.itemMarcador.Longitude}}"></marker>
            </ng-map>
        </div>
        <div class="modal-footer ">
            <button type="button" class="btn btn-primary  pull-right" ng-click="vmMapa.salvar()">
                <span class="fa fa-cloud-upload" aria-hidden="true"></span> @TextosPaginas.Geral_Salvar
            </button>
            <button type="button" class="btn btn-primary" ng-click="vmMapa.close()">
                <span class="fa fa-close" aria-hidden="true">@TextosPaginas.Cancelar</span>
            </button>
        </div>
    </script>

    <script type="text/ng-template" id="EditaCusto.html">

        <div class="modal-header">
            <span>@TextosPaginas.Refeicao_AdicionaCusto</span>
        </div>
        <div class="modal-body" ng-init="itemGastoEdit.load()">
            <div class="box box-primary" ng-include="'Sistema/GastoEdicao'">

            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" ng-click="itemGastoEdit.abrirMapa()">
                <span class="fa fa-map-marker" aria-hidden="true">@TextosPaginas.Posicao</span>
            </button>
            <button type="button" class="btn btn-primary" ng-click="itemGastoEdit.close()">
                <span class="fa fa-close" aria-hidden="true">@TextosPaginas.Cancelar</span>
            </button>

            <button type="button" class="btn btn-primary  pull-right" ng-click="itemGastoEdit.salvar()">
                <span class="fa fa-save" aria-hidden="true"></span> @TextosPaginas.Geral_Salvar
            </button>
        </div>
    </script>


</section><!-- /.content -->
                                            <tr>
                                                <th class="col-xs-2">
                                                    @TextosPaginas.HotelEvento_Entrada
                                                </th>
                                                <th class="col-xs-2">
                                                </th>
                                                <th class="col-xs-2">
                                                    @TextosPaginas.HotelEvento_Saida
                                                </th>
                                                <th class="col-xs-2">
                                                </th>

                                                <th class="col-xs-2">
                                                </th>
                                            </tr>
                                        </thead>
                                        <tr ng-repeat='itemCusto in itemEdit.itemHotel.Eventos' ng-if="!itemCusto.DataExclusao && itemCusto.IdentificadorUsuario == itemEdit.loggedUser.Codigo">
                                            <td>

<section class="content-header">
    <h1>
        @TextosPaginas.Home_Hotel

    </h1>
    <ol class="breadcrumb">

        <li>@TextosPaginas.Home_Menu</li>
        <li class="active">@TextosPaginas.Home_Hotel</li>
    </ol>

</section>
<!-- Main content -->
<section class="content" ng-init="itemHotel.load()">
    <!-- Default box -->
    <!-- general form elements -->
    <div class="box box-primary">
        <div class="overlay" ng-show="itemHotel.loading">
            <i class="fa fa-refresh fa-spin text-gray"></i>
        </div>

        <div class="box collapsed-box">
            <div class="box-header with-border">
                <h3 class="box-title"><i class="fa fa-filter"></i>&nbsp;@TextosPaginas.Foto_Filtros</h3>

                <div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-plus"></i></button>
                </div>
            </div>
            <div class="box-body">
                <div class="row">

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Hotel_Nome</label>
                        <input type="text" class="form-control" ng-model="itemHotel.filtro.Nome" maxlength="100" empty-to-null>
                    </div>

                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Hotel_CheckInPrevistoDe</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataInicioDe" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataInicioDe"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Hotel_CheckInPrevistoAte</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataInicioAte" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataInicioAte"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Hotel_CheckOutPrevistoDe</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataFimDe" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataFimDe"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Hotel_CheckOutPrevistoAte</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataFimAte" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataFimAte"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Foto_IdentificadorCidade</label>
                        <select ng-model="itemHotel.itemCidade" class="form-control" ng-options="obj.Nome for obj in itemHotel.ListaCidades track by obj.Identificador">
                            <option value="">@TextosPaginas.Selecione</option>
                        </select>
                    </div>

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Atracao_Situacao</label>

                        <div class="form-inline">

                            <label class="radio">
                                <input type="radio" class="radio-inline" name="radSituacao" value="1" ng-model="itemHotel.filtro.Situacao" />

                                <label style="padding-top:5px">

                                    @TextosPaginas.Hotel_Presente
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radSituacao" value="2" ng-model="itemHotel.filtro.Situacao" /><label>
                                    @TextosPaginas.Hotel_Passado
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radSituacao" value="3" ng-model="itemHotel.filtro.Situacao" />
                                <label>
                                    @TextosPaginas.Hotel_Futuro
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radSituacao" value="4" ng-model="itemHotel.filtro.Situacao" />
                                <label>
                                    @TextosPaginas.Hotel_Todos
                                </label>
                            </label>
                        </div>

                    </div>
                </div>




            </div><!-- /.box-body -->
            <div class="box-footer">


                <button class="btn btn-primary  pull-right" ng-click="itemHotel.filtraDado()">
                    <span class="fa fa-search" aria-hidden="true"></span> @TextosPaginas.Pesquisar
                </button>

            </div>
        </div>

        <div class="box-footer">
            <a role="button" class="btn btn-primary " title="@TextosPaginas.Adicionar" ng-click="itemHotel.CriarNovoHotel()">
                <span class="fa fa-plus" aria-hidden="true"></span>
            </a>
        </div>
    </div><!-- /.box -->



    <div class="box">
        <div class="overlay" ng-show="itemHotel.loading">
            <i class="fa fa-refresh fa-spin text-gray"></i>
        </div>
        <div ng-repeat='item in itemHotel.ListaDados' class="box " ng-class="{ 'collapsed-box': item.Identificador }" class-validation ng-controller="HotelEditCtrl as itemEdit">
            <div class="overlay" ng-show="itemEdit.loading">
                <i class="fa fa-refresh fa-spin text-gray"></i>
            </div>

            <div class="box-header with-border">
                <a data-widget="collapse" data-toggle="tooltip" ng-click="itemEdit.load(item)" style="cursor:pointer">
                    <i class="fa fa-plus" ng-class="{ 'fa-minus': !item.Identificador }"></i>


                    <h3 class="box-title">{{item.Nome}}</h3>

                </a>
            </div>

            <div class="box-body" ng-init="itemEdit.ajustaInicio(item)">

                <div class="row" ng-show="itemEdit.messages.length > 0">
                    <div class="col-md-12">
                        <div class="callout callout-danger">
                            <h4>@TextosPaginas.Geral_ValidarErros</h4>
                            <ul>
                                <li ng-repeat="erro in itemEdit.messages">
                                    <span ng-bind="erro.Mensagem"></span>
                                </li>
                            </ul>

                        </div>
                    </div>

                </div>

                <div class="row">
                    @*<div class="col-md-6">
            <label>@TextosPaginas.Atracao_EnderecoBase</label>
            <input type="text" class="form-control" ng-model="item.Endereco" maxlength="100" empty-to-null>

        </div>*@
                    <div class="col-md-6">
                        <label>@TextosPaginas.Hotel_Nome</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemHotel.Nome" maxlength="100" empty-to-null ng-if="(!itemEdit.itemHotel.Latitude || !itemEdit.itemHotel.Longitude) && !itemEdit.position.lat">
                        <input ng-if="(itemEdit.itemHotel.Latitude && itemEdit.itemHotel.Longitude) || itemEdit.position.lat " type="text" ng-model="itemEdit.itemHotel.Nome" uib-typeahead="address.name for address in itemEdit.CarregarDadosGoogleApi($viewValue) " typeahead-loading="itemEdit.loadingLocations" class="form-control" typeahead-on-select="itemEdit.SelecionarPlacesGoogle($item, $model, $label)">

                    </div>
                    <div class="col-md-3">
                        <label>@TextosPaginas.Hotel_Raio</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemHotel.Raio" maxlength="3" empty-to-null integer-input>

                    </div>
                    <div class="col-md-3">
                        <label>@TextosPaginas.Hotel_Movel</label>
                        <input type="checkbox" ng-model="itemEdit.Movel"  class="checkbox">
                    </div>
                    <div class="col-md-3">
                        <label>@TextosPaginas.Hotel_EntradaPrevista</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemEdit.itemHotel.EntradaPrevista" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemEdit.itemHotel.EntradaPrevista"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label>@TextosPaginas.Hotel_SaidaPrevista</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemEdit.itemHotel.SaidaPrevista" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemEdit.itemHotel.SaidaPrevista"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>

                    <div class="col-md-3">
                        <label>@TextosPaginas.Hotel_CheckIn</label>
                        <input type="checkbox" ng-model="itemEdit.VisitaIniciada" ng-click="itemEdit.AjustarHoraChegada();" class="checkbox">
                    </div>
                    <div class="col-md-3" ng-if="itemEdit.VisitaIniciada">
                        <label>@TextosPaginas.Hotel_CheckOut</label>
                        <input type="checkbox" ng-model="itemEdit.VisitaConcluida" ng-click="itemEdit.AjustarHoraPartida();" class="checkbox">
                    </div>

                </div>
                <div class="row" ng-if="itemEdit.VisitaIniciada">
                    <div class="col-md-3">
                        <label>@TextosPaginas.Hotel_DataEntrada</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemEdit.itemHotel.DataEntrada" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemEdit.itemHotel.DataEntrada"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label>&nbsp;</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemHotel.strHoraEntrada" input-mask="{mask: 'h:s:s'}" empty-to-null>

                    </div>

                    <div class="col-md-3" ng-if="itemEdit.VisitaConcluida">
                        <label>@TextosPaginas.Hotel_DataSaidia</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemEdit.itemHotel.DataSaidia" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemEdit.itemHotel.DataSaidia"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3" ng-if="itemEdit.VisitaConcluida">
                        <label>&nbsp;</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemHotel.strHoraSaida" input-mask="{mask: 'h:s:s'}" empty-to-null>

                    </div>
                </div>

                <div class="row" ng-show="itemEdit.VisitaIniciada">
                    <div class="col-md-12">
                        <label>@TextosPaginas.Hotel_Hospedes</label>

                        <div class="form-inline">
                            <label ng-repeat='itemParticipante in itemEdit.ListaParticipante' class="radio">
                                <input type="checkbox" ng-model="itemParticipante.Selecionado">
                                <label style="padding-top:5px">
                                    {{itemParticipante.Nome}}
                                </label>
                            </label>

                        </div>

                    </div>
                </div>
                <div class="row" ng-show="itemEdit.VisitaConcluida">
                    <div class="col-md-12" ng-if="itemEdit.VisitaConcluida && itemEdit.VerificarParticipoHotel()">
                        <label>@TextosPaginas.Atracao_Nota</label>
                        <div star-rating ng-model="itemEdit.ItemAvaliacao.Nota" max="5"></div>
                    </div>
                    <div class="col-md-12" ng-if="itemEdit.VisitaConcluida && itemEdit.VerificarParticipoHotel()">
                        <label>@TextosPaginas.Atracao_Comentario</label>
                        <textarea class="form-control" rows="5" ng-model="itemEdit.ItemAvaliacao.Comentario"></textarea>

                    </div>
                </div>
                <div class="row" ng-show="item.Identificador">
                    <div class="col-xs-12">
                        <ul class="nav nav-tabs">
                            <li class="active"><a data-toggle="tab" data-target="#custos{{item.Identificador}}" style="cursor:pointer">@TextosPaginas.Atracao_Custos</a></li>
                            <li><a data-toggle="tab" data-target="#midia{{item.Identificador}}" style="cursor:pointer" ng-click="itemEdit.RecarregarFotos()">@TextosPaginas.Atracao_Midias</a></li>
                            <li ng-if="itemEdit.itemOriginal.DataEntrada"><a data-toggle="tab" data-target="#evento{{item.Identificador}}" style="cursor:pointer">@TextosPaginas.Hotel_TempoHotel</a></li>

                        </ul>

                        <div class="tab-content" style="height:140px">
                            <div id="custos{{item.Identificador}}" class="tab-pane fade in active ">
                                <div class="box" style="overflow-y:auto; height:140px">
                                    <table class="table-condensed " style="width:100%; ">
                                        <thead>
                                            <tr>
                                                <th class="col-xs-3">
                                                    @TextosPaginas.Gasto_Descricao
                                                </th>
                                                <th class="col-xs-3">
                                                    @TextosPaginas.Gasto_Data
                                                </th>
                                                <th class="col-xs-3">
                                                    @TextosPaginas.Gasto_Moeda
                                                </th>
                                                <th class="col-xs-2">
                                                    @TextosPaginas.Gasto_Valor
                                                </th>

                                                <th class="col-xs-1">

                                                </th>
                                            </tr>
                                        </thead>
                                        <tr ng-repeat='itemCusto in itemEdit.itemHotel.Gastos' ng-if="!itemCusto.DataExclusao">
                                            <td>
                                                {{itemCusto.ItemGasto.Descricao}}
                                            </td>
                                            <td>
                                                {{itemCusto.ItemGasto.Data | date:'dd/MM/yyyy'}}
                                            </td>
                                            <td>
                                                {{itemCusto.ItemGasto.MoedaSigla}}
                                            </td>
                                            <td>
                                                {{itemCusto.ItemGasto.Valor | number:'2'}}
                                            </td>
                                            <td>
                                                <button type="button" class="btn btn-danger small" ng-click="itemEdit.RemoverCusto( itemCusto)"><span class="fa fa-trash" aria-hidden="true"></span></button>

                                            </td>
                                        </tr>
                                    </table>

                                    <div class="box-footer">
                                        <a role="button" class="btn-primary btn-sm pull-right" title="@TextosPaginas.Adicionar" ng-click="itemEdit.AbrirGasto();">
                                            <span class="fa fa-plus" aria-hidden="true"></span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <div id="midia{{item.Identificador}}" class="tab-pane fade in ">
                                <div class="dropzone" file-dropzone="[image/png, image/jpeg, image/gif]"

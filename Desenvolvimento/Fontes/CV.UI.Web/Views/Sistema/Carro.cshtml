<section class="content-header">
    <h1>
        @TextosPaginas.Home_Carro

    </h1>
    <ol class="breadcrumb">

        <li>@TextosPaginas.Home_Menu</li>
        <li class="active">@TextosPaginas.Home_Carro</li>
    </ol>

</section>
<!-- Main content -->
<section class="content" ng-init="itemHotel.load()">
    <!-- Default box -->
    <!-- general form elements -->
    <div class="box box-primary">
        <div class="overlay" ng-show="itemHotel.loading">
            <i class="fa fa-refresh fa-spin text-gray"></i>
        </div>

        <div class="box collapsed-box">
            <div class="box-header with-border">
                <h3 class="box-title"><i class="fa fa-filter"></i>&nbsp;@TextosPaginas.Foto_Filtros</h3>

                <div class="box-tools pull-right">
                    <button class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse"><i class="fa fa-plus"></i></button>
                </div>
            </div>
            <div class="box-body">
                <div class="row">

                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Carro_Descricao</label>
                        <input type="text" class="form-control" ng-model="itemHotel.filtro.Nome" maxlength="100" empty-to-null>
                    </div>
                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Carro_Modelo</label>
                        <input type="text" class="form-control" ng-model="itemHotel.filtro.Tipo" maxlength="100" empty-to-null>
                    </div>
                    <div class="form-group col-md-6">
                        <label>@TextosPaginas.Carro_Locadora</label>
                        <input type="text" class="form-control" ng-model="itemHotel.filtro.Comentario" maxlength="100" empty-to-null>
                    </div>
                </div>
                <div class="row">

                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Carro_DataRetiradaDe</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataInicioDe" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataInicioDe"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Carro_DataRetiradaAte</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataInicioAte" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataInicioAte"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Carro_DataDevolucaoDe</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataFimDe" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataFimDe"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                    <div class="form-group col-md-3">
                        <label>@TextosPaginas.Carro_DataDevolucaoAte</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemHotel.filtro.DataFimAte" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemHotel.filtro.DataFimAte"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemHotel.Idioma()  }" />
                            </ul>
                        </div>
                    </div>
                </div>

            </div><!-- /.box-body -->
            <div class="box-footer">


                <button class="btn btn-primary  pull-right" ng-click="itemHotel.filtraDado()">
                    <span class="fa fa-search" aria-hidden="true"></span> @TextosPaginas.Pesquisar
                </button>

            </div>
        </div>

        <div class="box-footer">
            <a role="button" class="btn btn-primary " title="@TextosPaginas.Adicionar" ng-click="itemHotel.CriarNovoHotel()">
                <span class="fa fa-plus" aria-hidden="true"></span>
            </a>
        </div>
    </div><!-- /.box -->



    <div class="box">
        <div class="overlay" ng-show="itemHotel.loading">
            <i class="fa fa-refresh fa-spin text-gray"></i>
        </div>
        <div ng-repeat='item in itemHotel.ListaDados' class="box " ng-class="{ 'collapsed-box': item.Identificador }" class-validation ng-controller="CarroEditCtrl as itemEdit">
            <div class="overlay" ng-show="itemEdit.loading">
                <i class="fa fa-refresh fa-spin text-gray"></i>
            </div>

            <div class="box-header with-border">
                <a data-widget="collapse" data-toggle="tooltip" ng-click="itemEdit.load(item)" style="cursor:pointer">
                    <i class="fa fa-plus" ng-class="{ 'fa-minus': !item.Identificador }"></i>


                    <h3 class="box-title">{{item.Descricao}}</h3>

                </a>
            </div>

            <div class="box-body" ng-init="itemEdit.ajustaInicio(item)">

                <div class="row" ng-show="itemEdit.messages.length > 0">
                    <div class="col-md-12">
                        <div class="callout callout-danger">
                            <h4>@TextosPaginas.Geral_ValidarErros</h4>
                            <ul>
                                <li ng-repeat="erro in itemEdit.messages">
                                    <span ng-bind="erro.Mensagem"></span>
                                </li>
                            </ul>

                        </div>
                    </div>

                </div>

                <div class="row">

                    <div class="col-md-6 " ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Descricao}">
                        <label>@TextosPaginas.Carro_Descricao</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.Descricao" maxlength="200" empty-to-null>
                    </div>

                    <div class="col-md-6" ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Modelo}">
                        <label>@TextosPaginas.Carro_Modelo</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.Modelo" maxlength="100" empty-to-null>
                    </div>

                    <div class="col-md-3 " ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Alugado}">
                        <label>@TextosPaginas.Carro_KM</label>
                        <div class="form-inline">

                            <label class="radio">
                                <input type="radio" class="radio-inline" name="radUnidade" value="1" ng-model="itemEdit.itemViagemAerea.KM" />

                                <label style="padding-top:5px">

                                    @TextosPaginas.Geral_KM
                                </label>
                            </label>
                            <label class="radio" style="margin-left:10px">
                                <input type="radio" class="radio-inline" name="radUnidade" value="0" ng-model="itemEdit.itemViagemAerea.KM" /><label>
                                    @TextosPaginas.Geral_MI
                            </label>
                        </label>
                    </div>

                </div>

                <div class="col-md-3 " ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Alugado}">
                    <label>@TextosPaginas.Carro_Alugado</label>
                    <input type="checkbox" ng-model="itemEdit.itemViagemAerea.Alugado" class="checkbox">

                </div>

                <div class="col-md-3 " ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Alugado}">
                    <label>@TextosPaginas.Carro_Alugado</label>
                    <input type="checkbox" ng-model="itemEdit.itemViagemAerea.Alugado" class="checkbox">

                </div>

                <div class="col-md-6" ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.Locadora}" ng-if="itemEdit.itemViagemAerea.Alugado">
                    <label>@TextosPaginas.Carro_Locadora</label>
                    <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.Locadora" maxlength="50" empty-to-null>
                </div>

                <div class="col-md-6" ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.DataRetirada}" ng-if="itemEdit.itemViagemAerea.Alugado">
                    <label>@TextosPaginas.Carro_DataRetirada</label>
                    <div class="dropdown">
                        <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                            <div class="input-group">
                                <input date-input ng-model="itemEdit.itemViagemAerea.DataRetirada" class="form-control" input-mask="{mask: 'd/m/y'}"
                                       datapattern="dd/MM/yyyy">
                                <span class="input-group-addon">
                                    <i class="glyphicon glyphicon-calendar"></i>
                                </span>
                            </div>
                        </a>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                            <datetimepicker data-ng-model="itemEdit.itemViagemAerea.DataRetirada"
                                            data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                        </ul>
                    </div>
                </div>

                <div class="col-md-6" ng-class="{ 'has-error':itemViagemAereaEdit.CamposInvalidos.DataDevolucao}" ng-if="itemEdit.itemViagemAerea.Alugado">
                    <label>@TextosPaginas.Carro_DataRetirada</label>
                    <div class="dropdown">
                        <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                            <div class="input-group">
                                <input date-input ng-model="itemEdit.itemViagemAerea.DataDevolucao" class="form-control" input-mask="{mask: 'd/m/y'}"
                                       datapattern="dd/MM/yyyy">
                                <span class="input-group-addon">
                                    <i class="glyphicon glyphicon-calendar"></i>
                                </span>
                            </div>
                        </a>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                            <datetimepicker data-ng-model="itemEdit.itemViagemAerea.DataDevolucao"
                                            data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()  }" />
                        </ul>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-md-10">
                    <h3>@TextosPaginas.Carro_Retirada</h3>
                </div>
                <div class="col-md-2">
                    <a role="button" class="btn btn-primary " title="@TextosPaginas.Posicao" ng-click="itemEdit.SelecionarPosicao(itemEdit.itemPontoOrigem)">
                        <span class="fa fa-map-marker" aria-hidden="true"></span>
                    </a>
                </div>               

                <div class="col-md-3">
                    <label>@TextosPaginas.Carro_Data</label>
                    <div class="dropdown">
                        <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                            <div class="input-group">
                                <input date-input ng-model="itemEdit.itemPontoOrigem.Data" class="form-control" input-mask="{mask: 'd/m/y'}"
                                       datapattern="dd/MM/yyyy">
                                <span class="input-group-addon">
                                    <i class="glyphicon glyphicon-calendar"></i>
                                </span>
                            </div>
                        </a>
                        <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                            <datetimepicker data-ng-model="itemEdit.itemPontoOrigem.Data"
                                            data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()   }" />
                        </ul>
                    </div>
                </div>
                <div class="col-md-3" >
                    <label>&nbsp;</label>
                    <input type="text" class="form-control" ng-model="itemEdit.itemPontoOrigem.strHora" input-mask="{mask: 'h:s:s'}" empty-to-null>

                </div>

                <div class="col-md-6">
                    <label>@TextosPaginas.Carro_Odometro</label>
                    <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.OdometroInicial" maxlength="7" empty-to-null integer-input>

                </div>


            </div>


                <div class="row">
                    <div class="col-md-10">
                        <h3>@TextosPaginas.Carro_Devolucao</h3>
                    </div>
                    <div class="col-md-2">
                        <a role="button" class="btn btn-primary " title="@TextosPaginas.Posicao" ng-click="itemEdit.SelecionarPosicao(itemEdit.itemPontoDestino)">
                            <span class="fa fa-map-marker" aria-hidden="true"></span>
                        </a>
                    </div>

                    <div class="col-md-3">
                        <label>@TextosPaginas.Carro_Data</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="itemEdit.itemPontoDestino.Data" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="itemEdit.itemPontoDestino.Data"
                                                data-datetimepicker-config="{  minView:'day' , langCode:itemViagemAerea.Idioma()   }" />
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <label>&nbsp;</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemPontoDestino.strHora" input-mask="{mask: 'h:s:s'}" empty-to-null>

                    </div>

                    <div class="col-md-6">
                        <label>@TextosPaginas.Carro_Odometro</label>
                        <input type="text" class="form-control" ng-model="itemEdit.itemViagemAerea.OdometroFinal" maxlength="7" empty-to-null integer-input>

                    </div>


                </div>

            <div class="row">
                <div class="col-md-12">
                    <label>@TextosPaginas.Carro_Ocupantes</label>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="form-inline">
                        <label ng-repeat='itemParticipante in itemEdit.ListaParticipante' class="radio">
                            <input type="checkbox" ng-model="itemParticipante.Selecionado">
                            <label style="padding-top:5px">
                                {{itemParticipante.Nome}}
                            </label>
                        </label>

                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12" ng-if="itemEdit.VerificarParticipoViagemAerea() && itemEdit.itemPontoDestino.Data && itemEdit.itemViagemAerea.Alugado">
                    <label>@TextosPaginas.Refeicao_Nota</label>
                    <div star-rating ng-model="itemEdit.ItemAvaliacao.Nota" max="5"></div>
                </div>
                <div class="col-md-12" ng-if="itemEdit.VerificarParticipoViagemAerea() && itemEdit.itemPontoDestino.Data && itemEdit.itemViagemAerea.Alugado">
                    <label>@TextosPaginas.Refeicao_Comentario</label>
                    <textarea class="form-control" rows="5" ng-model="itemEdit.ItemAvaliacao.Comentario"></textarea>

                </div>
            </div>
            <div class="row" ng-show="item.Identificador">
                <div class="col-xs-12">
                    <ul class="nav nav-tabs">
                        <li class="active"><a data-toggle="tab" data-target="#custos{{item.Identificador}}" style="cursor:pointer">@TextosPaginas.Refeicao_Custos</a></li>
                        <li ><a data-toggle="tab" data-target="#reabastecimentos{{item.Identificador}}" style="cursor:pointer">@TextosPaginas.Home_Reabastecimento</a></li>

                    </ul>

                    <div class="tab-content" style="height:140px">
                        <div id="custos{{item.Identificador}}" class="tab-pane fade in active ">
                            <div class="box" style="overflow-y:auto; height:140px">

                                <table class="table-condensed " style="width:100%; ">
                                    <thead>
                                        <tr>
                                            <th class="col-xs-3">
                                                @TextosPaginas.Gasto_Descricao
                                            </th>
                                            <th class="col-xs-3">
                                                @TextosPaginas.Gasto_Data
                                            </th>
                                            <th class="col-xs-3">
                                                @TextosPaginas.Gasto_Moeda
                                            </th>
                                            <th class="col-xs-2">
                                                @TextosPaginas.Gasto_Valor
                                            </th>

                                            <th class="col-xs-1">

                                            </th>
                                        </tr>
                                    </thead>
                                    <tr ng-repeat='itemCusto in itemEdit.itemViagemAerea.Gastos' ng-if="!itemCusto.DataExclusao">
                                        <td>
                                            {{itemCusto.ItemGasto.Descricao}}
                                        </td>
                                        <td>
                                            {{itemCusto.ItemGasto.Data | date:'dd/MM/yyyy'}}
                                        </td>
                                        <td>
                                            {{itemCusto.ItemGasto.MoedaSigla}}
                                        </td>
                                        <td>
                                            {{itemCusto.ItemGasto.Valor | number:'2'}}
                                        </td>
                                        <td>
                                            <button type="button" class="btn btn-danger small" ng-click="itemEdit.RemoverCusto( itemCusto)"><span class="fa fa-trash" aria-hidden="true"></span></button>

                                        </td>
                                    </tr>
                                </table>

                                <div class="box-footer">
                                    <a role="button" class="btn-primary btn-sm pull-right" title="@TextosPaginas.Adicionar" ng-click="itemEdit.AbrirGasto();">
                                        <span class="fa fa-plus" aria-hidden="true"></span>
                                    </a>
                                </div>
                            </div>
                        </div>

                        <div id="reabastecimentos{{item.Identificador}}" class="tab-pane fade in active ">
                            <div class="box" style="overflow-y:auto; height:140px">

                                <table class="table-condensed " style="width:100%; ">
                                    <thead>
                                        <tr>
                                            <th class="col-xs-4">
                                                @TextosPaginas.Reabastecimento_Data
                                            </th>
                                            <th class="col-xs-3">
                                                @TextosPaginas.Reabastecimento_QuantidadeReabastecida
                                            </th>      
                                            <th class="col-xs-3">
                                                @TextosPaginas.Reabastecimento_Valor
                                            </th>  
                                            <th class="col-xs-2">

                                            </th>
                                        </tr>
                                    </thead>
                                    <tr ng-repeat='itemCusto in itemEdit.itemViagemAerea.Reabastecimentos' ng-if="!itemCusto.DataExclusao">
                                        <td>
                                            {{itemCusto.Data | date:'dd/MM/yyyy'}}
                                        </td>
                                        <td>
                                            {{itemCusto.QuantidadeReabastecida | number:'2'}}
                                        </td>                                     
                                      
                                        <td>
                                            {{itemCusto.Valor | number:'2'}}
                                        </td>
                                        <td>
                                            <button type="button" class="btn btn-danger small" ng-click="itemEdit.RemoverCusto( itemCusto)"><span class="fa fa-trash" aria-hidden="true"></span></button>

                                        </td>
                                    </tr>
                                </table>

                                <div class="box-footer">
                                    <a role="button" class="btn-primary btn-sm pull-right" title="@TextosPaginas.Adicionar" ng-click="itemEdit.AbrirReabastecimento();">
                                        <span class="fa fa-plus" aria-hidden="true"></span>
                                    </a>
                                </div>
                            </div>
                        </div>

                    </div>

                </div>
            </div>
        </div>
        <div class="box-footer">

            <a role="button" class="btn btn-primary pull-right" title="@TextosPaginas.Geral_Salvar" ng-click="itemEdit.save()">
                <span class="fa fa-save" aria-hidden="true"></span>
            </a>

            <a role="button" class="btn btn-primary pull-right" title="@TextosPaginas.Excluir" ng-click="itemEdit.VerificarExclusao()">
                <span class="fa fa-trash-o" aria-hidden="true"></span>
            </a>



        </div>
    </div>


</div>


<script type="text/ng-template" id="modalDelete.html">
    <div class="modal-header">
        <span>{{vmDelete.MensagemConfirmacao}}</span>
    </div>
    <div class="modal-body">
        <button type="button" class="btn btn-info btn-lg btn-block" ng-click="vmDelete.confirmar()">{{vmDelete.TextoBotaoOK}}</button>
        <button type="button" class="btn btn-default btn-lg btn-block" ng-click="vmDelete.close()">{{vmDelete.TextoBotaoCancel}}</button>
    </div>
</script>


<script type="text/ng-template" id="editaImagem.html">

    <div class="modal-header">
        <span>@TextosPaginas.Foto_EditaMidia</span>
    </div>
    <div class="modal-body" ng-init="vmEdit.load()">
        <div class="box box-primary">
            <div class="box-body">
                <div class="row" ng-show="vmEdit.messages.length > 0">
                    <div class="col-md-12">
                        <div class="callout callout-danger">
                            <h4>@TextosPaginas.Geral_ValidarErros</h4>
                            <ul>
                                <li ng-repeat="erro in vmEdit.messages">
                                    <span ng-bind="erro.Mensagem"></span>
                                </li>
                            </ul>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group  col-md-12" ng-class="{ 'has-error': vmEdit.CamposInvalidos.Comentario }">
                        <label>@TextosPaginas.Foto_Comentario</label>
                        <input type="text" class="form-control" ng-model="vmEdit.itemFoto.ItemFoto.Comentario" empty-to-null>
                    </div>

                </div>
                <div class="row">
                    <div class="form-group col-md-6" ng-class="{ 'has-error': vmEdit.CamposInvalidos.Data }">
                        <label for="txtDataInicio">@TextosPaginas.Foto_Data</label>
                        <div class="dropdown">
                            <a class="dropdown-toggle" id="A1" role="button" data-toggle="dropdown">
                                <div class="input-group">
                                    <input date-input ng-model="vmEdit.itemFoto.ItemFoto.Data" class="form-control" input-mask="{mask: 'd/m/y'}"
                                           datapattern="dd/MM/yyyy">
                                    <span class="input-group-addon">
                                        <i class="glyphicon glyphicon-calendar"></i>
                                    </span>
                                </div>
                            </a>
                            <ul class="dropdown-menu" role="menu" aria-labelledby="dLabel">
                                <datetimepicker data-ng-model="vmEdit.itemFoto.ItemFoto.Data"
                                                data-datetimepicker-config="{  minView:'day', langCode: vmEdit.Idioma() }" />
                            </ul>
                        </div>
                    </div>

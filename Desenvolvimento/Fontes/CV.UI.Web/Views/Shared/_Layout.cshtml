<!DOCTYPE html>
<html data-ng-app='CV'>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@TextosPaginas.TituloAplicacao</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body class="hold-transition skin-blue-light sidebar-mini" ng-cloak>

    <div class="wrapper" ng-controller="AccountCtrl as account" ng-init="account.load()">
        <!--Caso logado-->
        <div>


        </div>
        <header class="main-header">
            <!-- Logo -->
            <a href="/#/home" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>@TextosPaginas.TituloAplicacaoReduzido</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>@TextosPaginas.TituloAplicacao</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top" role="navigation">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button" ng-show="$root.isLogged">
                    <span class="sr-only"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav" ng-show="$root.isLogged">

                        <li class="dropdown user user-menu">
                            <a href="javascript:void(0)" class="dropdown-toggle" data-toggle="dropdown">

                                <span class="hidden-xs" ng-bind="account.user.Login"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <p>
                                        <span ng-bind="account.user.Nome"></span>
                                        <small> @TextosPaginas.UltimoAcesso <span ng-bind="account.user.strUltimoAcesso"></span></small>
                                    </p>

                                    

                                </li>

                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <a href="javascript:void(0)" class="btn btn-default btn-flat" ng-click="account.alterarSenha()">@TextosPaginas.Home_AlterarSenha</a>
                                    </div>
                                    <div class="pull-right">
                                        <a href="javascript:void(0)" class="btn btn-default btn-flat" ng-click="account.logout()">@TextosPaginas.Sair</a>
                                    </div>
                                </li>
                            </ul>
                        </li>

                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar ng-hide" ng-show="$root.isLogged">
            <section class="sidebar">
                <ul class="nav sidebar-menu" id="side-menu">

                    <li class="treeview">
                        <a ui-sref="home()"><i class="fa fa-dashboard fa-fw"></i> <span> @TextosPaginas.Home_Dashboard</span></a>
                    </li>
                    <li class="treeview">
                        <a href="javascript:void(0)"><i class="fa fa-lock fa-fw"></i><span>@TextosPaginas.Home_Menu</span><i class="fa fa-angle-left pull-right"></i></a>
                        <ul class="nav nav-second-level treeview-menu">
                            							<li ><a ui-sref="Usuario()"><span>@TextosPaginas.Home_Usuario</span></a></li>
							<li ><a ui-sref="Reabastecimento()"><span>@TextosPaginas.Home_Reabastecimento</span></a></li>
							<li ><a ui-sref="Amigo()"><span>@TextosPaginas.Home_Amigo</span></a></li>
							<li ><a ui-sref="AporteDinheiro()"><span>@TextosPaginas.Home_AporteDinheiro</span></a></li>
							<li ><a ui-sref="Atracao()"><span>@TextosPaginas.Home_Atracao</span></a></li>
							<li ><a ui-sref="CalendarioPrevisto()"><span>@TextosPaginas.Home_CalendarioPrevisto</span></a></li>
							<li ><a ui-sref="Carro()"><span>@TextosPaginas.Home_Carro</span></a></li>
							<li ><a ui-sref="Cidade()"><span>@TextosPaginas.Home_Cidade</span></a></li>
							<li ><a ui-sref="CidadeGrupo()"><span>@TextosPaginas.Home_CidadeGrupo</span></a></li>
							<li ><a ui-sref="Comentario()"><span>@TextosPaginas.Home_Comentario</span></a></li>
							<li ><a ui-sref="CotacaoMoeda()"><span>@TextosPaginas.Home_CotacaoMoeda</span></a></li>
							<li ><a ui-sref="Foto()"><span>@TextosPaginas.Home_Foto</span></a></li>
							<li ><a ui-sref="Gasto()"><span>@TextosPaginas.Home_Gasto</span></a></li>
							<li ><a ui-sref="Hotel()"><span>@TextosPaginas.Home_Hotel</span></a></li>
							<li ><a ui-sref="ListaCompra()"><span>@TextosPaginas.Home_ListaCompra</span></a></li>
							<li ><a ui-sref="Pais()"><span>@TextosPaginas.Home_Pais</span></a></li>
							<li ><a ui-sref="Posicao()"><span>@TextosPaginas.Home_Posicao</span></a></li>
							<li ><a ui-sref="Loja()"><span>@TextosPaginas.Home_Loja</span></a></li>
							<li ><a ui-sref="Refeicao()"><span>@TextosPaginas.Home_Refeicao</span></a></li>
							<li ><a ui-sref="RequisicaoAmizade()"><span>@TextosPaginas.Home_RequisicaoAmizade</span></a></li>
							<li ><a ui-sref="Sugestao()"><span>@TextosPaginas.Home_Sugestao</span></a></li>
							<li ><a ui-sref="Viagem()"><span>@TextosPaginas.Home_Viagem</span></a></li>
							<li ><a ui-sref="ViagemAerea()"><span>@TextosPaginas.Home_ViagemAerea</span></a></li>

                        </ul>
                    </li>
                    
                </ul>

            </section>
        </aside>

        <!--<div ng-class="{'content-wrapper':$root.isLogged}">-->
        <div class="content-wrapper" ng-class="{'nomargin':$root.isLogged == false}">
            <div error-manager></div>
            <div data-ui-view>
            </div>
        </div>
        @RenderBody()

        <!--<div class="navbar navbar-fixed-bottom">-->
        <!--footer class="main-footer" ng-class="{'nomargin':$root.isLogged == false}">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; 2015-2016 <a href="http://www.architettura.com.br">Architettura</a>.</strong>
        </footer-->
        <!--</div>-->

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)
</body>
</html>